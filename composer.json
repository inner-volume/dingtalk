{
  "name": "openphp/dingtalk",
  "description": "钉钉自定义机器人接入[https://open.dingtalk.com/document/robots/custom-robot-access]",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "zzqqw",
      "email": "zhiqiangwang@aliyun.com"
    }
  ],
  "require": {
    "php": ">7.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0|~7.0"
  },
  "autoload": {
    "psr-4": {
      "Openphp\\Dingtalk\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Openphp\\Dingtalk\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "tests": [
      "php vendor/bin/phpunit"
    ]
  }
}
